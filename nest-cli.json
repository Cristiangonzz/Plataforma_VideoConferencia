{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/video-conferencia/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/video-conferencia/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/video-conferencia",
  "projects": {
    "video-conferencia": {
      "type": "application",
      "root": "apps/video-conferencia",
      "entryFile": "main",
      "sourceRoot": "apps/video-conferencia/src",
      "compilerOptions": {
        "tsConfigPath": "apps/video-conferencia/tsconfig.app.json"
      }
    },
    "usuario": {
      "type": "application",
      "root": "apps/usuario",
      "entryFile": "main",
      "sourceRoot": "apps/usuario/src",
      "compilerOptions": {
        "tsConfigPath": "apps/usuario/tsconfig.app.json"
      }
    },
    "lista-usuario": {
      "type": "application",
      "root": "apps/lista-usuario",
      "entryFile": "main",
      "sourceRoot": "apps/lista-usuario/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lista-usuario/tsconfig.app.json"
      }
    }
  }
}